num=int(input("enter lenghth list"))
lst1=[]
lst2=[]
tup =()
for i in range(num):
    lst1.append(input("enter value "))
print(lst1)
for i in set(lst1):
    count=0
    tup = ()
    for j in range(0,len(lst1)):
        if(i==lst1[j]):
            count=count+1;
    tup=(i,count)

    lst2.append(tup)
    del tup
print(lst2)
