lst=[]
for i in range(int(input("enter range of 1st list " ))):
    lst.append(int(input()))
print("1st list isk  : ",lst)
lst1=[]
for i in range(int(input("enter range of seocund List "))):
    lst1.append(int(input()))
print("2nd list is : ",lst1)
for i in lst1:
    lst.append(i)
print("Apending both list is : ",lst)

