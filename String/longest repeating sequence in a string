str= input("enter string ")
strCount=0;
for i in set(str):
    count=0;
    for j in str:
        if i==j:
            count=count+1
        if strCount<count:
            strCount=count;
            str2 =i;
print("maximum repated string is : %s and %d \n time reapeat :"%(str2,strCount))
            
