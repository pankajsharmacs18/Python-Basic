def largestThreeeNo(num1,num2,num3):
    if(num2>num1 and num2>num2):
        return num2
    elif (num1>num2 and num1>num3):
        return num1
    else:
        return num3
num1=int(input("Enter three number :"))
num2=int(input("Enter three number :"))
num3=int(input("Enter three number :"))
print("largest num is :")
print(largestThreeeNo(num1,num2,num3))
